<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.txa.dao.UserDao">

    <insert id="insertUser" parameterType="com.txa.domain.User">
        INSERT INTO user1 (id,name,sex,age)
        VALUES (#{id,jdbcType=VARCHAR},#{name,jdbcType=VARCHAR},#{sex,jdbcType=VARCHAR},#{age,jdbcType=INTEGER})
    </insert>

</mapper>
